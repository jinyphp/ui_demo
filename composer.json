{
    "name": "jiny/ui_demo",
    "description": "jiny ui demo components",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "hojinlee",
            "email": "infohojin@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.3|^8.0",
        "jiny/html":"^0.9",
        "jiny/theme":"^0.9.6",
        "jiny/ui":"^0.9.1",
        "livewire/livewire": "^2.7"
    },
    "autoload": {
        "psr-4": {
            "Jiny\\UIDemo\\":"src/"
        },
        "files" : [
            "src/Helpers/Helper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Jiny\\UIDemo\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jiny\\UIDemo\\JinyUIDemoServiceProvider"
            ]
        }
    }

}
